{
    "name": "KnowledgeKubeDomainSelector",
    "version": "1.0",
    "description": "Works with KnowledgeKube to select the domain while running in debug",
    "manifest_version": 2,
    "permissions": [
        "storage",
        "webRequest",
        "webRequestBlocking",
        "https://localhost:5001/"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_popup": "popup.html"
    }
}